#!/bin/bash
oldcmd=`basename $0`
newcmd=`echo $oldcmd | rev`
echo
echo "Gah!! I'm the new $oldcmd command and I refuse to work today!!"
echo "Go away!! Leave me alone!!"
echo
echo "Sun-Hwa advises you spell $oldcmd backwards to get around "
echo "this command's bad attitude.  That is, try $newcmd instead." 
echo
exit
